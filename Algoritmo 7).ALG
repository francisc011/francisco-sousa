Algoritmo "conevrter_grausC_em_grausF"
 // Autor: Francisco Sousa
 // Data: 14-09-2017
 // Descrição: Conversão_de_C_em_F
Var
    Cº, Fº : real
    início
    escreva("Qual a temperatura em Cº?")
    leia(Cº)
    escreva("Qual atemperatura em Fº?")
    leia(Fº)
    Fº <- Cº * 3.875
    escreva("A temperatura em Fº é",F)
Fimalgoritmo
